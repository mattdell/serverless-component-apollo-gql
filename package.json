{
  "name": "front-end",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook",
    "build:analyze": "cross-env ANALYZE_BUNDLE=both yarn build",
    "build:analyze-client": "cross-env ANALYZE_BUNDLE=browser yarn build",
    "build:analyze-server": "cross-env ANALYZE_BUNDLE=server yarn build",
    "csslint": "sass-lint -c sass-lint.yml -v -q",
    "deploy": "cross-env SLS_DEBUG=* serverless deploy",
    "eslint": "eslint . --ext js,jsx,ts,tsx",
    "integration": "wdio wdio.local.js",
    "integration:ci": "wdio wdio.browserstack.js",
    "lighthouse:ci": "lhci autorun",
    "lint": "yarn eslint && yarn csslint",
    "prettier-check": "prettier --check \"src/**/*.ts*\" --debug-check",
    "prettify": "prettier --write --config ../../.prettierrc \"src/**/*.{ts,js,css,scss,tsx}\"",
    "start": "cross-env IS_LOCAL=* next",
    "start:debug": "cross-env IS_LOCAL=*  NODE_OPTIONS=\"--inspect\" next",
    "storybook": "start-storybook -p 6006 --ci",
    "test": "jest --verbose=true",
    "test:ci": "jest --ci --reporters=jest-junit --reporters=default --verbose=true --coverage --coverageReporters=cobertura --coverageReporters=html"
  },
  "workspaces": {
    "nohoist": [
      "**/serverless-*",
      "**/serverless-*/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test",
      "pre-commit": "pretty-quick --staged"
    }
  },
